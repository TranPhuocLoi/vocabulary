<div class="container mt-5">
  <div class="row">
    <div class="col">
      <p id="notice"><%= notice %></p>
      <!-- <%= @post %> -->
      <h3>
        <strong>New word:</strong>
        <%= @post.vocabulary %>
      </h3>
      <p>
        <strong>User create:</strong>
        <%= @post.user.username_def %>
      </p>
    </div>
  </div>
</div>
<div class="row">
  <div class="container text-center">
    <%= link_to 'Edit', edit_admin_post_path(@post),:class => 'btn btn-warning mt-4' %>
    <%= link_to 'Back', admin_posts_path,:class => 'btn btn-success mt-4' %>
  </div>
</div>
</div>