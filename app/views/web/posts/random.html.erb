<div class="container mt-5">
  <div class="row">
    <div class="col text-center">
      <% @posts.each do |post| %>
      <tr>
        <td>
          <h4>Auto vocabulary: <%= post.vocabulary %></h4>
        </td>
        <td>
          <p class="text-primary">
            User create: <%= post.user.username_def %></p>
        </td>
      </tr>

      <% end %>
      <%= link_to 'Generate Vocabulary', random_web_posts_path,:class => 'btn btn-warning mt-4' %>
    </div>
  </div>
</div>